{
  "name": "freelancetrack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "deploy": "npm run build && npm run db:push",
    "docker:build": "docker build -t freelancetrack .",
    "docker:run": "docker run -p 3000:3000 freelancetrack",
    "docker:compose": "docker-compose up -d",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "freelance",
    "tracking",
    "project-management"
  ],
  "author": "Aaron George Abraham",
